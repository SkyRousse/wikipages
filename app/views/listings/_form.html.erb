<%= render "layouts/errors", :object => @listing %>

<%= form_for [@category, @listing] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <%= f.label :website %>
  <%= f.text_field :website %>
  <%= f.label :hours %>
  <%= f.text_field :hours %>
  <%= f.submit %>
<% end %>
